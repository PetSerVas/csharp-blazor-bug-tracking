@page "/new-bug" 

<h3>Add New Bug</h3>

<EditForm Model="@AddBug">

    <input @bind="@AddBug.Title" placeholder="Enter Title" />
    <input @bind="@AddBug.Description" placeholder="Enter Description" />
    <input @bind="@AddBug.Priority" placeholder="Enter Priority" />

    <button type="submit">Add Bug</button>

</EditForm>

@code {
    public Bug AddBug = new Bug();
    

}

